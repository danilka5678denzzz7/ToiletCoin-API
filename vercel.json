{
  "version": 2,
  "builds": [
    {
      "src": "mini_app_backend.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"  <--- ИЗМЕНИТЕ ЭТУ СТРОКУ
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "mini_app_backend.py"
    }
  ]
}
