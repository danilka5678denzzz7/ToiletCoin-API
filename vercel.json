{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",   <--- ПУТЬ ДОЛЖЕН БЫТЬ ИЗМЕНЕН
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"  <--- ПУТЬ ДОЛЖЕН БЫТЬ ИЗМЕНЕН
    }
  ]
}
